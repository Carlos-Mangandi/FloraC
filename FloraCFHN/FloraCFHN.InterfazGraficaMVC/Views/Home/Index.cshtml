@{
    ViewData["Title"] = "Home Page";

}

@*<body style="background-color:#F2F4F4;">*@

<!DOCTYPE html>
<html>
<head>
    <title>W3.CSS Template</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Oswald">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open Sans">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <style>
        h1, h2, h3, h4, h5, h6 {
            font-family: "Oswald"
        }

        body {
            font-family: "Open Sans"
        }
    </style>
</head>
<body class="w3-light-grey" style="background-color:#F2F4F4;">

    <div class="w3-content" style="max-width:400px">

        <!-- Header -->
        <header class="w3-container w3-center w3-padding-48 w3-white">
            <h6 class="w3-jumbo w3-animate-top">Bienvenido a nuestra aplicación web<span class="w3-tag w3-jumbo w3-animate-top"> FLORA CFHN </span></h6>
        </header>

        <!-- Image header -->
        <header class="w3-display-container w3-wide" id="home">
            <img class="w3-image" src="https://i.pinimg.com/564x/cb/82/bf/cb82bf49016feadd76677c871ec2b0c0.jpg" width="400" height="400">           
        </header>
    </div>
</body>
</html>
